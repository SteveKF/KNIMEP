package com.newbrightidea.util;

import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.io.OutputStream;
import java.io.PrintWriter;

public class Test {

	public static void main(String[] args) {
		RTree<Integer> tree = new RTree<Integer>(3, 2, 2);
		tree.insert(new float[] { 1, 9 }, 0);
		tree.insert(new float[] { 2, 10 }, 0);
		tree.insert(new float[] { 4, 8 }, 0);
		tree.insert(new float[] { 6, 7 }, 0);
		tree.insert(new float[] { 9, 10 }, 0);
		tree.insert(new float[] { 7, 5 }, 0);
		tree.insert(new float[] { 5, 6 }, 0);
		tree.insert(new float[] { 4, 3 }, 0);
		tree.insert(new float[] { 3, 2 }, 0);
		tree.insert(new float[] { 10, 4 }, 0);
		tree.insert(new float[] { 9, 1 }, 0);
		tree.insert(new float[] { 6, 2 }, 0);
		tree.insert(new float[] { 8, 3 }, 0);
		String html = tree.visualize();
		try (PrintWriter out = new PrintWriter("filename.html")) {
			out.println(html);
		}catch(FileNotFoundException e){
			e.printStackTrace();
		}
	}
}
