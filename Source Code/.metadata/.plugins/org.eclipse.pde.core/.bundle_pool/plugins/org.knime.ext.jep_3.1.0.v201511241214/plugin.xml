<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/manipulation/column/column-convert+replace" factory-class="org.knime.ext.jep.JEPNodeFactory" id="org.knime.ext.jep.JEPNodeFactory"/>
   </extension>
   
<extension point="org.eclipse.help.toc">
<toc extradir="html" file="tocs/manipulation_column_column-convert+replace$nodes.xml"/>
</extension>
</plugin>
