<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
     <extension
         point="org.knime.workbench.repository.nodes">
        <node
              category-path="/database/database-connector"
              factory-class="org.knime.bigdata.vertica.node.connector.VerticaConnectorNodeFactory"
              id="org.knime.bigdata.vertica.node.connector.VerticaConnectorNodeFactory">
        </node>
    </extension>
     <extension
           point="org.knime.core.DatabaseUtility">
        <databaseUtility
              class="org.knime.bigdata.vertica.utility.VerticaUtility"
              database="vertica">
        </databaseUtility>
     </extension>
     <extension
           point="org.knime.core.JDBCDriver">
        <jdbcDriver
              jarFile="lib/vertica-jdk5-6.1.3-0.jar">
        </jdbcDriver>
     </extension>
</plugin>
